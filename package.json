{
  "name": "isam-cli",
  "version": "1.0.0",
  "description": "Bluescape command line tool",
  "type": "commonJs",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "bs": "npm run build && node ./build/cli.js",
    "package:linux": "pkg . --no-bytecode --targets node14-linux-x64 --output bin/linux/bluescape",
    "package:macos": "pkg . --no-bytecode --targets node14-macos-x64 --output bin/macos/bluescape",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
    "format:check": "prettier --loglevel warn --check \"**/*.{ts,js,json,yaml}\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "test": "npm run test "
  },
  "main": "./build/cli.js",
  "bin": {
    "bluescape": "./build/cli.js",
    "bs": "./build/cli.js"
  },
  "pkg": {
    "scripts": "build/**/*.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "cli"
  ],
  "author": "sathishkumar.sethu@bluescape.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "5.0.1",
    "clear": "0.1.0",
    "clui": "0.3.6",
    "conf": "10.1.2",
    "inquirer": "8.2.4",
    "node-fetch": "3.2.4",
    "ora": "5.4.1",
    "yargs": "17.4.1"
  },
  "devDependencies": {
    "@types/clear": "0.1.2",
    "@types/configstore": "5.0.1",
    "@types/inquirer": "8.2.1",
    "@types/node": "17.0.31",
    "@types/yargs": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "nodemon": "2.0.16",
    "pkg": "5.6.0",
    "prettier": "2.6.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  }
}
